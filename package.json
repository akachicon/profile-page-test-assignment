{
  "name": "sunlight-test-assignment",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run --silent lint"
    }
  },
  "scripts": {
    "serve:dev": "next dev",
    "serve:prod": "next start",
    "build:prod": "next build",
    "deploy": "npm run build:prod && npm run serve:prod",
    "prettier": "prettier --check \"**/*.(js|mjs|jsx)\"",
    "prettier:fix": "prettier --write \"**/*.(js|mjs|jsx)\"",
    "eslint": "eslint --ext .js,.mjs,.jsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "next": "9.5.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "typescript": "^4.0.3"
  }
}
